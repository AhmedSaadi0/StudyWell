<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.study.mystudyapp">

    <application
        android:name=".MVVMApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_read"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_read"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.games.hanzi.HanziGameActivity" />
        <activity android:name=".ui.hsk.MainHskActivity" />
        <activity
            android:name=".ui.login.LogInActivity"
            android:theme="@style/AddTheme" />
        <activity
            android:name=".ui.main.addword.AddWordActivity"
            android:theme="@style/AddTheme" />
        <activity android:name=".ui.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>